{
    "fairMQOptions":
    {
        "device":
        {
            "id": "sampler",
            "channel":
            {
                "name": "output",
                "address": "tcp://*:48000",
                "type": "pub",
                "method": "bind",
                "rateLogging": "1"
            }
        },

        "device":
        {
            "id": "receiver",
            "channel":
            {
                "name": "data",
                "socket":
                {
                    "type": "sub",
                    "method": "connect",
                    "address": "tcp://localhost:48000",
                    "sndBufSize": "1000",
                    "rcvBufSize": "1000",
                    "rateLogging": "1"
                }
            },
            "channel":
            {
                "name": "control",
                "socket":
                {
                    "type": "sub",
                    "method": "connect",
                    "address": "tcp://localhost:48001",
                    "sndBufSize": "1000",
                    "rcvBufSize": "1000",
                    "rateLogging": "1"
                }
            }
        }

    }
}
